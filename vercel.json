{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.ts",
      "use": "@vercel/node"
    },
    {
      "src": "landing-pages/*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/landing-pages/(.*)",
      "dest": "/landing-pages/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    }
  ]
}